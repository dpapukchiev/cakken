{
    "name": "cakken",
    "version": "0.1.0",
    "private": false,
    "main": "lib/src/index",
    "author": {
        "name": "Daniel Papukchiev",
        "email": "dpapukchiev@gmail.com"
    },
    "license": "MIT",
    "repository": {
        "type": "github",
        "url": "https://github.com/dpapukchiev/cakken"
    },
    "scripts": {
        "test": "nyc mocha",
        "test:watch": "mocha -w ",
        "build": "rm -rf lib && tsc"
    },
    "devDependencies": {
        "ts-node": "^7.0.1",
        "typescript": "^3.1.1",
        "@types/chai": "^4.1.4",
        "@types/mocha": "^5.2.5",
        "@types/node": "^8.10.20",
        "@types/sinon": "^4.3.3",
        "chai": "^4.1.2",
        "mocha": "^5.2.0",
        "mocha-typescript": "1.1.12",
        "nyc": "^12.0.2",
        "sinon": "^4.5.0",
        "ts-loader": "^2.3.7",
        "typemoq": "^2.1.0",
        "webpack": "^3.12.0",
        "webpack-node-externals": "^1.6.0"
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "src/index.ts",
            "src/**/*.interface.ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "extension": [
            ".ts"
        ],
        "reporter": [
            "lcov",
            "text-summary",
            "html"
        ],
        "sourceMap": true,
        "instrument": true,
        "all": true
    }
}